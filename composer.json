{
    "name": "webeweb/bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Intregate Bootstrap theme with Symfony 2 and more",
    "keywords": [
        "bootstrap",
        "bundle",
        "symfony",
        "theme",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/bootstrap-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "psr/log": "~1.0",
        "symfony/asset": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/security-core": "^3.4|^4.0",
        "symfony/serializer": "^3.4|^4.0",
        "symfony/translation": "^3.4|^4.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-bundle": "^2.20",
        "webeweb/core-library": "^8.0"
    },
    "conflict": {
        "webeweb/jquery-datatables-bundle": "<3.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/orm": "^2.5",
        "monolog/monolog": "^1.18",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^3.4|^4.0",
        "symfony/templating": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/bootstrap-bundle/issues"
    }
}
