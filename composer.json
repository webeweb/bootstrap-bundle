{
    "name": "webeweb/bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Integrate Bootstrap theme with Symfony 4 and more",
    "keywords": [
        "bootstrap",
        "bundle",
        "symfony",
        "theme",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/bootstrap-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "symfony/asset": "^4.4|^5.0|^6.0",
        "symfony/form": "^4.4|^5.0|^6.0",
        "symfony/framework-bundle": "^4.4|^5.0|^6.0",
        "symfony/security-core": "^4.4|^5.0|^6.0",
        "symfony/translation": "^4.4|^5.0|^6.0",
        "twig/twig": "^2.10|3.0",
        "webeweb/core-bundle": "^3.22",
        "webeweb/core-library": "^8.58"
    },
    "conflict": {
        "webeweb/jquery-datatables-bundle": "<3.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0",
        "doctrine/doctrine-bundle": "^1.8.1|^2.0",
        "doctrine/orm": "^2.5.13",
        "masterminds/html5": "^2.6",
        "symfony/browser-kit": "^4.4|^5.0|^6.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^4.4|^5.0|^6.0",
        "symfony/twig-bundle": "^4.4|^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/bootstrap-bundle/issues"
    }
}
