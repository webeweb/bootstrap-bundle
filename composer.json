{
    "name": "webeweb/bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Intregate Bootstrap theme with Symfony 2 and more",
    "keywords": [
        "bootstrap",
        "bundle",
        "symfony",
        "theme",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/bootstrap-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "psr/log": "~1.0",
        "symfony/asset": "^2.7|^3.0|^4.0",
        "symfony/form": "^2.7|^3.0|^4.0",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "symfony/security-core": "^2.7|^3.0|^4.0",
        "symfony/serializer": "^2.7|^3.0|^4.0",
        "symfony/translation": "^2.7|^3.0|^4.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-library": "^5.1",
        "webeweb/core-bundle": "^1.3",
        "webeweb/syntaxhighlighter-bundle": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^7.0",
        "doctrine/orm": "^2.2",
        "doctrine/doctrine-bundle": "^1.0",
        "symfony/browser-kit": "^2.7|^3.0|^4.0",
        "symfony/monolog-bundle": "^2.7|^3.0|^4.0",
        "symfony/security-bundle": "^2.7|^3.0|^4.0",
        "symfony/templating": "^2.7|^3.0|^4.0",
        "symfony/twig-bundle": "^2.7|^3.0|^4.0"
    },
    "support": {
        "issues": "https://github.com/webeweb/bootstrap-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
