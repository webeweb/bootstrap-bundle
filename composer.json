{
    "name": "webeweb/bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Intregate Bootstrap theme with Symfony 2",
    "keywords": ["webeweb", "bundle", "symfony", "bootstrap", "theme"],
    "homepage": "http://github.com/webeweb/bootstrap-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "http://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "doctrine/common": "^2.0",
        "symfony/asset": "^2.6|^3.0",
        "symfony/form": "^2.6|^3.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "symfony/monolog-bundle": "^2.6|^3.0",
        "symfony/security-core": "^2.6|^3.0",
        "symfony/serializer": "^2.6|^3.0",
        "symfony/translation": "^2.6|^3.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-library": "^4.0",
        "webeweb/syntaxhighlighter-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "symfony/browser-kit": "^2.6|^3.0",
        "symfony/expression-language": "^2.6|^3.0",
        "symfony/security-bundle": "^2.6|^3.0",
        "symfony/templating": "^2.6|^3.0",
        "symfony/twig-bundle": "^2.6|^3.0"
    },
    "support": {
        "issues": "http://github.com/webeweb/bootstrap-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
