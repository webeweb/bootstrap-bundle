{# Resources/views/Wiki/twig-extension/component/label.html.twig #}
{% extends "@Bootstrap/Wiki/layout.html.twig" %}

{% import "@Bootstrap/Wiki/macros.html.twig" as wiki %}

{% block bootstrapContent %}
    {{ wiki.functionsTitle() }}
    {{ wiki.functionsList([
        "bootstrapLabelDanger",
        "bootstrapLabelDefault",
        "bootstrapLabelInfo",
        "bootstrapLabelPrimary",
        "bootstrapLabelSuccess",
        "bootstrapLabelWarning",
    ]) }}
    <div class="panel panel-default">
        {{ wiki.functionTitle("bootstrapLabelDanger") }}
        <div class="panel-body">
            {{ wiki.argumentsList([
                ["string", "content", "the content"],
            ]) }}
            {{ wiki.functionExample('{{ bootstrapLabelDanger({"content": "content"}) }}') }}
            {{ wiki.functionOutput(bootstrapLabelDanger({"content": "content"})) }}
        </div>
    </div>
    <div class="panel panel-default">
        {{ wiki.functionTitle("bootstrapLabelDefault") }}
