{# Resources/views/Wiki/twig-extension/css/typography.html.twig #}
{% extends "@Bootstrap/Wiki/layout.html.twig" %}

{% import "@Bootstrap/Wiki/macros.html.twig" as wiki %}

{% block bootstrapContent %}
    {{ wiki.functionsTitle() }}
    {{ wiki.functionsList([
        "bootstrapBold",
        "bootstrapDeleted",
        "bootstrapHeading1",
        "bootstrapHeading2",
        "bootstrapHeading3",
        "bootstrapHeading4",
        "bootstrapHeading5",
        "bootstrapHeading6",
        "bootstrapInserted",
        "bootstrapItalic",
        "bootstrapMarked",
        "bootstrapSmall",
        "bootstrapStrikethrough",
        "bootstrapUnderlined",
    ]) }}
    <div class="panel panel-default">
        {{ wiki.functionTitle("bootstrapBold") }}
        <div class="panel-body">
            {{ wiki.argumentsList([
                ["string", "content", "the content"],
            ]) }}
            {{ wiki.functionExample('{{ bootstrapBold({"content": "content"}) }}') }}
            {{ wiki.functionOutput(bootstrapBold({"content": "content"})) }}
        </div>
    </div>
    <div class="panel panel-default">
        {{ wiki.functionTitle("bootstrapDeleted") }}
